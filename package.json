{
  "name": "fixture-loader",
  "version": "0.2.4",
  "description": "Load fixture files from a specified dir",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint .",
    "test": "NODE_ENV=test istanbul cover _mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@springworks/test-harness": "1.0.3",
    "babel": "5.8.29",
    "babel-eslint": "4.1.5",
    "coveralls": "2.11.4",
    "eslint": "1.10.0",
    "eslint-config-springworks": "4.0.2",
    "eslint-plugin-mocha": "1.1.0",
    "istanbul": "0.4.0",
    "mocha": "2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Springworks/node-fixture-loader.git"
  },
  "keywords": [
    "fixture"
  ],
  "author": "Springworks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Springworks/node-fixture-loader/issues"
  },
  "homepage": "https://github.com/Springworks/node-fixture-loader#readme",
  "engines": {
    "npm": ">=2.7.0"
  },
  "dependencies": {
    "xml2js": "0.4.15"
  }
