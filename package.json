{
  "name": "fixture-loader",
  "version": "0.2.5",
  "description": "Load fixture files from a specified dir",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint .",
    "test": "NODE_ENV=test istanbul cover _mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@springworks/test-harness": "1.2.3",
    "babel": "5.8.29",
    "babel-eslint": "6.0.0",
    "coveralls": "2.11.9",
    "eslint": "2.4.0",
    "eslint-config-springworks": "7.0.1",
    "eslint-plugin-import": "1.3.0",
    "eslint-plugin-mocha": "2.0.0",
    "eslint-plugin-springworks": "1.1.2",
    "istanbul": "0.4.2",
    "mocha": "2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Springworks/node-fixture-loader.git"
  },
  "keywords": [
    "fixture"
  ],
  "author": "Springworks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Springworks/node-fixture-loader/issues"
  },
  "homepage": "https://github.com/Springworks/node-fixture-loader#readme",
  "engines": {
    "npm": ">=2.7.0"
  },
  "dependencies": {
    "xml2js": "0.4.16"
  }
