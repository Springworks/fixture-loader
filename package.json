{
  "name": "fixture-loader",
  "description": "Load fixture files from a specified dir",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "commit": "commit-wizard",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "NODE_ENV=test istanbul cover _mocha"
  },
  "dependencies": {
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "@springworks/test-harness": "1.3.13",
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-register": "6.11.5",
    "coveralls": "2.11.11",
    "eslint": "3.1.1",
    "eslint-config-springworks": "7.0.4",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-mocha": "4.2.0",
    "eslint-plugin-springworks": "2.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "2.5.3",
    "pre-git": "3.10.0",
    "semantic-release": "4.3.5"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Springworks/node-fixture-loader.git"
  },
  "keywords": [
    "fixture"
  ],
  "author": "Springworks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Springworks/node-fixture-loader/issues"
  },
  "homepage": "https://github.com/Springworks/node-fixture-loader#readme"
}
